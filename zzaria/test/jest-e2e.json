{
  "roots": [
      "src"
   ],
  "moduleDirectories": [
      "node_modules",
      "src"
   ],
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
  }
}
